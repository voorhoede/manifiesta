{
  "name": "manifiesta",
  "version": "1.0.0",
  "description": "Configure and preview your Progressive Web App Manifest.",
  "homepage": "https://voorhoede.github.io/manfiesta/",
  "license": "MIT",
  "author": "De Voorhoede",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:app": "node build/build.js",
    "build:guide": "build-storybook -c config/storybook -o dist/guide/",
    "dev": "run-p dev:*",
    "dev:app": "PORT=6264 node build/dev-server.js",
    "dev:guide": "start-storybook -p 6267 -c config/storybook -s ./",
    "dev:services": "micro-dev services.js -p ${PORT:-6265}",
    "lint": "eslint --ext .js,.vue src",
    "prestart": "npm run build",
    "start": "http-server './dist/' -c-1 -o -p 6266",
    "test": "run-s lint"
  },
